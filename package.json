{
  "name": "cerebral-view-inferno",
  "version": "1.1.5",
  "description": "React View layer package for Cerebral",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config webpack.build.config.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cerebral-legacy/cerebral-view-inferno.git"
  },
  "keywords": [
    "react",
    "cerebral",
    "flux",
    "mvc"
  ],
  "author": "Christian Alfoni <christianalfoni@gmail.com>",
  "contributors": [
    "Aleksey Guryanov <gurianov@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cerebral-legacy/cerebral-view-inferno/issues"
  },
  "homepage": "https://github.com/cerebral-legacy/cerebral-view-inferno#readme",
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "commitizen": "^2.5.0",
    "conventional-changelog": "0.0.17",
    "cz-customizable": "^2.7.1",
    "ghooks": "^1.0.3",
    "semantic-release": "^4.3.5",
    "standard": "^7.1.2",
    "validate-commit-msg": "^1.1.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "inferno": "^0.7.16",
    "inferno-dom": "^0.7.16",
    "inferno-create-element": "^0.7.16",
    "inferno-component": "^0.7.16"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "release": {
    "analyzeCommits": "./sr-prerelease.js"
  }
}
